extends layout


block content
    h1.text-info #{title}
    .row
        .col-12
            form(action="/ledStatus/form", method="post")
                .input-group
                    label(for="status")
                    input.form-control(type="text" name="status" placeholder="Estado Led")
                .input-group
                    input.btn.btn-success(type="submit" value="Agregar")